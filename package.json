  "scripts": {
    "dev": "nodemon",
    "build": "rmdir /s /q dist && tsc",
    "start": "node dist/server.js",
    "test": "jest --config jest.config.ts --coverage",
    "migrate:create": "prisma migrate dev --create-only",
